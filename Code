#include<iostream>
using namespace std;
int max_of_four(int a,int b,int c,int d)
{
    int e{};
    if(a>b)
      e=a;
    else
        e=b;
    if(e<c)
        e=c;
    if(e<d)
        e=d;
    return e;
}


int main() {
    cout<<"Enter the numbers you want to compare :";
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);

    return 0;
}
